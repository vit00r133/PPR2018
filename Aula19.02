!#/bin/bash
clear

read -p "Ip: " data
echo $data | grep -E '^[0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5]$'
if [ $? == 0 ]; then
        echo "Ip Disponivel"
else
        echo "Ip Ocupado"
fi

